<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN"
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <type id="zip">
    <file>
      <contentType>application/zip</contentType>
    </file>
  </type>
  <type id="binary">
    <file>
      <contentType>application/binary</contentType>
    </file>
  </type>
  <group name="localization">
    <setting name="file">
      <type>
        <file>
          <contentType>application/xml</contentType>
        </file>
      </type>
    </setting>
    <setting name="context">
      <type>
	<enum> 
	  <option><value>Web</value></option>
	  <option><value>Gui</value></option>
	</enum>
      </type>
      <value>Web</value>
    </setting>
    <setting name="language">
      <type>
        <enum>
          <option><value>Catalan</value></option>
          <option><value>Cestina</value></option>
          <option><value>Dansk</value></option>
          <option><value>Deutsch</value></option>
          <option><value>English</value></option>
          <option><value>English(UK)</value></option>
          <option><value>Espanol</value></option>
          <option><value>Francais</value></option>
          <option><value>Italiano</value></option>
          <option><value>Nederlands</value></option>
          <option><value>Norsk</value></option>
          <option><value>Poliski</value></option>
          <option><value>Portugues</value></option>
          <option><value>Russian</value></option>
          <option><value>Slovencina</value></option>
          <option><value>Suomi</value></option>
          <option><value>Svenska</value></option>
          <option><value>Turkce</value></option>
        </enum>
      </type>
      <value>Italiano</value>
    </setting>  

    <setting name="version">
      <type>
        <enum>
          <option><value>7.3.X</value></option>
          <option><value>8.4.X</value></option>
        </enum>
      </type>
      <value>7.3.X</value>
    </setting>
  </group>
  <group name="firmware" hidden="yes">
    <setting name="allowMultipleUploads" hidden="yes">
      <type>
        <boolean></boolean>
      </type>
      <value>1</value>
    </setting>
  </group>
</model>
