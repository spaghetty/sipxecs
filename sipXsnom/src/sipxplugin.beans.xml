<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  <bean id="snomModel" abstract="true" parent="abstractPhoneModel" class="org.sipfoundry.sipxconfig.phone.snom.SnomModel">
    <property name="beanId" value="snom" />
    <property name="defaultProfileLocation" ref="httpProfileLocation" />
    <property name="profileTemplate" value="snom/snom.vm" />
    <property name="vendor" value="SNOM" />
    <property name="restartSupported" value="true" />
    <property name="versions">
      <list>
        <ref local="org.sipfoundry.sipxconfig.phone.snom.SnomModel.VER_7_3_X"/>
        <ref local="org.sipfoundry.sipxconfig.phone.snom.SnomModel.VER_8_4_X"/>
        <ref local="org.sipfoundry.sipxconfig.phone.snom.SnomModel.VER_8_7_X"/>
      </list>
    </property>
  </bean>
  <bean id="org.sipfoundry.sipxconfig.phone.snom.SnomModel.VER_7_3_X"
        class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
  <bean id="org.sipfoundry.sipxconfig.phone.snom.SnomModel.VER_8_4_X"
        class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
  <bean id="org.sipfoundry.sipxconfig.phone.snom.SnomModel.VER_8_7_X"
        class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />

  <bean id="snom300" parent="snomModel">
    <property name="label" value="Snom 300" />
    <property name="maxLineCount" value="4" />
    <property name="supportedFeatures">
      <set>
        <value>intercom</value>
      </set>
    </property>
  </bean>
  <bean id="snom320" parent="snomModel">
    <property name="label" value="Snom 320" />
    <property name="maxLineCount" value="12" />
    <property name="supportedFeatures">
      <set>
        <value>intercom</value>
      </set>
    </property>
  </bean>
  <bean id="snom360" parent="snomModel">
    <property name="label" value="Snom 360" />
    <property name="maxLineCount" value="12" />
    <property name="supportedFeatures">
      <set>
        <value>intercom</value>
        <value>blf</value>
      </set>
    </property>
  </bean>
  <bean id="snom370" parent="snomModel">
    <property name="label" value="Snom 370" />
    <property name="maxLineCount" value="12" />
    <property name="supportedFeatures">
      <set>
        <value>intercom</value>
        <value>blf</value>
      </set>
    </property>
  </bean>
  <bean id="snom" class="org.sipfoundry.sipxconfig.phone.snom.SnomPhone" scope="prototype" parent="abstractPhone">
    <property name="speedDialManager" ref="speedDialManager" />
    <property name="uploadManager" ref="uploadManager" />
  </bean>

   <bean id="snomM3Model" abstract="true" parent="abstractPhoneModel">
    <property name="beanId" value="snomM3Phone" />
    <property name="modelDir" value="snom-m3" />
    <property name="profileTemplate" value="snom-m3/snom_m3.vm" />
    <property name="vendor" value="SNOM" />
    <property name="restartSupported" value="true" />
  </bean>

  <bean id="snomM3" parent="snomM3Model">
    <property name="label" value="Snom m3" />
    <property name="maxLineCount" value="8" />
    <property name="supportedFeatures">
      <set>
      </set>
    </property>
  </bean>

  <bean id="snomM3Phone" class="org.sipfoundry.sipxconfig.phone.snom.SnomM3Phone" scope="prototype" parent="abstractPhone">
  </bean>
  <bean id="snomLanguage" class="org.sipfoundry.sipxconfig.upload.UploadSpecification">
    <property name="beanId" value="snomUpload" />
    <property name="modelFilePath" value="snom/upload.xml" />
    <property name="label" value="Language File For Snom Phone" />
  </bean>
  <bean id="snomUpload" class="org.sipfoundry.sipxconfig.phone.snom.SnomUpload"
    scope="prototype" parent="upload">
    <property name="destinationDirectory">
      <value>${sysdir.phone}/profile/docroot/</value>
    </property>
  </bean>
 <bean id="langUploadListener" class="org.sipfoundry.sipxconfig.phone.snom.LangUploadListener">
    <property name="phoneContext" ref="phoneContext" />
  </bean>

</beans>
